{
  "name": "mlsolutions/chartjs-widget",
  "description": "A chartjs widget for laravel nova-dashboard.",
  "keywords": [
    "laravel",
    "nova",
    "chartjs",
    "dashboard",
    "nova-dashboard"
  ],
  "authors": [],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "MlSolutions\\ChartJsWidget\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MlSolutions\\ChartJsWidget\\WidgetServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require-dev": {
    "nova-kit/nova-packages-tool": "^2.0"
  }
}
